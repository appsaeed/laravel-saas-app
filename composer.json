{
  "name": "appsaeed/webapp",
  "description": "apsaeed spa - campain  Marketing Application",
  "keywords": [
    "framework",
    "laravel",
    "campaign",
    "appsaeed",
    "marketing"
  ],
  "license": "MIT",
  "require": {
    "php": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-zip": "*",
    "arcanedev/no-captcha": "^13.0",
    "arielmejiadev/larapex-charts": "^2.1",
    "authorizenet/authorizenet": "^2.0",
    "barryvdh/laravel-translation-manager": "^0.6.5",
    "beyondcode/laravel-websockets": "^1.14",
    "fruitcake/laravel-cors": "^2.2",
    "guzzlehttp/guzzle": "^7.4.2",
    "intervention/image": "^2.7",
    "laravel/framework": "^9.12.2",
    "laravel/helpers": "^1.5",
    "laravel/legacy-factories": "^1.3",
    "laravel/sanctum": "^2.15",
    "laravel/socialite": "^5.5",
    "laravel/tinker": "^2.7",
    "laravel/ui": "^3.4.5",
    "pusher/pusher-php-server": "^7.2",
    "tanmuhittin/laravel-google-translate": "^2.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/horizon": "^5.9.7",
    "laravel/sail": "^1.0.1",
    "laravel/telescope": "^4.9",
    "phpunit/phpunit": "^9.5.20",
    "spatie/laravel-ignition": "^1.2.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform-check": false
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-ide-helper",
        "laravel/telescope"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/Helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan horizon:publish --ansi"
    ]
  }
}
